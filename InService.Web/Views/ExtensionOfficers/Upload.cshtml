
@section subnav{
    <div class="navbar-nav">
        <div class="nav-item"><a class="nav-link" href="@Url.Index()"><i class="mdi mdi-chevron-left"></i>Extension officers</a></div>
        <div class="nav-item"><a class="nav-link" href="#"><i class="mdi mdi-plus"></i> Upload</a></div>
    </div>
}
<h3 class="font-weight-light">Upload file</h3>
<h5 class="small"><i class="mdi mdi-asterisk text-danger"></i>Upload only excel file</h5>
<div class="row">
    <div class="col-md-7">
        @using (Html.BeginMultiPartForm())
        {
            @Html.AntiForgeryToken()
            <div class="card shadow">
                <div class="card-body">
                    @Html.ValidationSummary(true, "", new { @class = "text-danger small" })

                    <div class="form-group row">
                        <label class="col-md-4 form-control-label">Extension officer file</label>
                        <div class="col-md-8">
                            <input type="file" class="form-control" name="file" required accept=".xlsx" />
                            @Html.ValidationMessage("file", new { @class = "text-danger" })
                        </div>
                    </div>
                    <hr />
                    <div class="mt-2 text-right">
                        <button class="btn btn-primary">Submit <i class="mdi mdi-checkbox-marked-circle"></i></button>
                    </div>
                </div>
            </div>
        }
    </div>
</div>





